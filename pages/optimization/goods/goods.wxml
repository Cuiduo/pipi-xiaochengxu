<view class="btn_jump">
  <view class='btn_item' bindtap='jumpTo'>
    <image src='../../../images/shucai.png'></image>
  </view>
  <view class='btn_item' bindtap='jumpTo'>
    <image src='../../../images/shuiguo.png'></image>
  </view>
  <view class='btn_item' bindtap='jumpTo' >
    <image src='../../../images/roulei.png'></image>
