/* pages/car/index/index.wxss */
.scrollView{border-top: 1px solid rgba(244,244,244,1); position: absolute; top: 0px; left: 0px; bottom: 0px; right: 0px; }
.scrollTop{position: absolute; top: 0rpx; left: 0rpx; right: 0rpx; bottom: 104rpx; overflow: hidden; overflow-y: auto; -webkit-overflow-scrolling: touch;}
.food{height: auto;}
.foodBox{display: flex;align-items: center;border-bottom: 1px solid rgba(244,244,244,1);height: 112rpx; line-height: 112rpx; padding: 0rpx 40rpx;}
.foodSelect{display: flex;}
.foodSelect image{width: 40rpx;height: 40rpx; margin: auto;}
.foodBox .title{margin-left: 20rpx;font-size: 32rpx;font-weight:bold;color:rgba(51,51,51,1);}
.foodBox .price{font-size:26rpx;font-weight:bold;color:rgba(136,136,136,1);margin-left: 40rpx;}
.foodBox .price text{
  font-weight: normal;
}
.foodList{
  display: flex;
  flex-direction: column;
  border-bottom: 1rpx solid rgba(244,244,244,1);
}
.foodListTop{
  display: flex;
  align-items: flex-start;
  padding: 24rpx 40rpx 0rpx 50rpx;
}
.foodListTop .listPic{
  width: 176rpx;
  height: 176rpx;
}
.foodListTop .listDes{
  flex: 1;
  margin-left: 24rpx;
  padding-bottom: 26rpx;
}
.listDesTitle{
  font-size:26rpx;
  font-weight:500;
  color:rgba(51,51,51,1);
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 380rpx;
}
.listFat{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  display: inline-flex;
  margin-top: 10rpx;
}
.listFat text{
  height:34rpx;
  line-height:34rpx;
  background:rgba(242,242,242,1);
  border-radius:24rpx;
  padding: 0 16rpx;
  font-size:18rpx;
  font-weight:400;
  color:rgba(136,136,136,1);
  margin-right: 8rpx;
}
.listFat text:last-child{margin-right: 0}
.listDesBtm{display: flex;justify-content: space-between; margin-top: 42rpx;}
.listDesBtm_left{display: flex; align-items: flex-end; font-weight:500;}
.listDesBtm_left_txt1{font-size:24rpx;color:rgba(241,83,31,1);}
.listDesBtm_left_txt1 text{font-size:36rpx;}
.listDesBtm_left_txt2{font-size:18rpx;color:rgba(178,178,178,1); margin-left: 12rpx;padding-bottom: 6rpx; text-decoration: line-through;}
.listDesBtm_right{display: flex; align-items: center;}
.listDesBtm_right text{font-size:30rpx;font-weight:500;color:rgba(35,35,35,1); width: 66rpx; text-align: center;}
.listDesBtm_right image{width: 44rpx; height: 44rpx;}
.foodListBtm{
  padding: 14rpx 72rpx 14rpx 50rpx;
  font-size: 22rpx;
  font-weight:400;
  line-height: 32rpx;
  color:rgba(51,51,51,1);
  position: relative;
  /* display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden */
}
.foodListBtm::after{content: ''; position: absolute;top: 0rpx;right: 0rpx;left: 250rpx;height: 1px;background:rgba(244,244,244,1);}
.foodListBtm .foodTips{font-size:20rpx;background:rgba(241,83,31,0.1);border-radius: 26rpx;color:rgba(241,83,31,1);padding: 0 16rpx;display: inline-block;margin-right: 12rpx;}
.shortIcon{position: absolute; right: 44rpx; bottom: 0rpx;}
.shortIcon image{width: 44rpx; height: 44rpx;}

.bottom{position: absolute;left: 0rpx;bottom: 0rpx;right: 0rpx;height:104rpx; line-height: 104rpx;background:rgba(255,255,255,1);box-shadow:0px -1px 10px rgba(0,0,0,0.05); display: flex; padding-left: 38rpx; justify-content: space-between;}
.totalName{font-size:26rpx;font-family:PingFang SC;font-weight:bold;color:rgba(241,83,31,1);}
.bottomBtn{width:180rpx; background:#FFC162; font-size:15px;font-family:PingFang SC;font-weight:bold; color:rgba(255,255,255,1);text-align: center;}

/* 去请客 分享 */
.popBg{position: absolute; left: 0; top: 0; right: 0; bottom: 0; background:rgba(0,0,0,.4); z-index: 10}
.toGuest{position: absolute; left: 0; bottom: 0; right: 0;background:rgba(255,255,255,1);box-shadow:0px -5px 10px rgba(0,0,0,0.3);border-radius:12px 12px 0px 0px;}
.guestList{display: flex; padding: 36rpx 40rpx; border-bottom: 1px solid rgba(244,244,244,1);
font-family:PingFang SC;font-weight:400; align-items: center;}
.guestName{ flex: 1; white-space: nowrap; overflow: hidden; text-overflow: ellipsis;
font-size:32rpx;color:rgba(51,51,51,1);}
.guestBtn1, .guestBtn2{width:140rpx; height:52rpx; line-height: 52rpx; background:rgba(241,83,31,1); border-radius:4px; text-align: center; font-size:26rpx; color:rgba(255,255,255,1); margin-left: 28rpx;}
.guestBtn2{ background:rgba(255,180,66,1);}
/* 组合选择名字 */
.groupbox{width:630rpx; height:450rpx; background:rgba(255,255,255,1);border-radius:8px; position: absolute; left: 50%; margin-left: -315rpx; top: 50%; margin-top: -225rpx; padding: 0 34rpx; box-sizing: border-box;}
.groupbox image{width: 56rpx; height: 56rpx; position: absolute; top: -68rpx; right: -10rpx;}
.shareTitle{font-size:30rpx;font-family:PingFang SC;font-weight:400;color:rgba(51,51,51,1); text-align: center; padding: 60rpx 0 48rpx 0;}
.shareName{display: flex; flex-wrap: wrap; margin-left: -2%;}
.shareName view{height:60rpx; line-height: 60rpx;background:rgba(249,249,249,1);border-radius:4px; text-align: center; font-size:26rpx;font-family:PingFang SC;font-weight:400;color:rgba(51,51,51,1); width: 31.33%; margin-left: 2%; margin-top: 12rpx;}
.toShare{height: 100rpx; line-height: 100rpx;border-top:1px solid rgba(244,244,244,1); margin-top: 60rpx; text-align: center; font-size:30rpx;font-family:PingFang SC;font-weight:bold;color:rgba(241,83,31,1);}



